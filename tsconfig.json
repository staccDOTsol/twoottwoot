{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2020",
    "module": "commonjs",
    "declaration": true,
    "outDir": "dist/server",
    "strict": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "noEmitOnError": true,
    "sourceRoot": "src/server",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "typeRoots": ["src/server/typings"],
  },
  "exclude": ["node_modules", "dist", "src/client"],
  "include": ["src/server"]
}
